{
    "name": "rhyslees/jetstream-team-profile-photo",
    "description": "This is my package jetstream-team-profile-photo",
    "keywords": [
        "RhysLees",
        "laravel",
        "jetstream-team-profile-photo"
    ],
    "homepage": "https://github.com/rhyslees/jetstream-team-profile-photo",
    "license": "MIT",
    "authors": [
        {
            "name": "Rhys Lees",
            "email": "43909932+RhysLees@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "laravel/jetstream": "^2.12",
        "livewire/livewire": "^2.10",
        "spatie/laravel-package-tools": "^1.13.0"
    },
    "require-dev": {
        "laravel/pint": "^1.5",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "2.x-dev",
        "pestphp/pest-plugin-laravel": "2.x-dev",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "RhysLees\\JetstreamTeamProfilePhoto\\": "src",
            "RhysLees\\JetstreamTeamProfilePhoto\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RhysLees\\JetstreamTeamProfilePhoto\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RhysLees\\JetstreamTeamProfilePhoto\\JetstreamTeamProfilePhotoServiceProvider"
            ],
            "aliases": {
                "JetstreamTeamProfilePhoto": "RhysLees\\JetstreamTeamProfilePhoto\\Facades\\JetstreamTeamProfilePhoto"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
